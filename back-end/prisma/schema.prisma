// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model post {
  id              Int           @id @default(autoincrement())
  content         String?
  named_entities  Json
  fetch_record_id Int
  fetch_record    fetch_record? @relation(fields: [fetch_record_id], references: [id])
}

model fetch_record {
  id         Int      @id @default(autoincrement())
  fetch_time DateTime
  posts      post[]
}
